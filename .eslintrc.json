{
  "extends": [
    "eslint:recommended",
    "react-app",
    "plugin:react-hooks/recommended",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "prettier",
    "react-hooks"
  ],
  "rules": {
    "prettier/prettier": [
      "warn"
    ]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  }
}
